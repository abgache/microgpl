{
    "webhook_url": "",
    "model_path": "model/model.pth",
    "data_path": "data/tiny_sheakespeare.txt",
    "json_data_path": "data/dataset.json",
    "dataset_loading_size": 1000000,
    "tokenizer":{
        "vocab_size": 10000,
        "path": "model/tokenizer.json",
        "special_tokens": {
            "<PAD>": 1,
            "<UNK>": 2,
            "<BOS>": 3,
            "<EOS>": 4
        }
    },
    "embedding":{
        "vector_dim": 256,
        "main_model_path": "model/embedding_model.pth",
        "json_table_path": "model/embedding_table.json",
        "dnn":{
            "input_size": 10000,
            "num_epochs": 10,
            "batch_size": 32,
            "learning_rate": 0.005
        }
    },
    "attention":{
        "context_window": 64,
        "num_heads": 2,
        "head_dim": {
            "query": 128,
            "key": 128,
            "value": 128
        },
        "matrix_path":{
            "W_Q": "model/W_Q.json",
            "W_K": "model/W_K.json",
            "W_V": "model/W_V.json",
            "W_O": "model/W_O.json"
        },
        "masking_value": -1e9
    },
    "ffn":{
        "model_path": "model/ffn.pth",
        "num_epochs": 25,
        "batch_size": 32,
        "learning_rate": 0.001
    },
    "version": "0.1.0"
}